{
  "name": "protocol-lists",
  "version": "0.0.1",
  "private": true,
  "description": "The Protocol Lists specification",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*",
    "./*.protocollist.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/buttonwood-protocol/buttonwood-protocol-list.git"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig-dist.json && node build/build.js",
    "lint": "yarn eslint && yarn prettier:list-different",
    "lint:fix": "yarn eslint:fix && yarn prettier",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --fix --ext .ts,.tsx",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:list-different": "prettier --config .prettierrc --list-different \"**/*.{js,json,md,sol,ts}\"",
    "start": "yarn build",
    "test": "mocha -r ts-node/register 'test/**/*test.ts'"
  },
  "engines": {
    "node": ">=14.17"
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@types/lodash": "^4.14.190",
    "@uniswap/token-lists": "^1.0.0-beta.27",
    "json-diff-ts": "^1.2.5",
    "lodash": "^4.x"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9.1.3",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
